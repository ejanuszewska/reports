<systemstats>
    <date></date>            <!-- dd.mm.yyyy -->
    <weekday></weekday>      <!-- full weekday name -->
    <time></time>            <!-- hh:mm:ss -->
    <cpuload></cpuload>      <!—- current total CPU load -->
    <memload></memload>      <!—- current total memory load -->
    <user>                   <!-- multiply user block for every currently logged user -->
       <username></username>
       <process>             <!-- multiply process block for every process of this user -->
           <pid></pid>      
           <cmd></cmd>       <!—- command/application name -->
       </process>
   </user>
</systemstats>
